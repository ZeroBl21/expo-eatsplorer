{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
    "baseUrl": ".",
      "@/*": [
        "./*"
      ],
     "@components/*": ["./components/*"],
      "@screens/*": ["./screens/*"],
      "@lib/*": ["./lib/*"],
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
